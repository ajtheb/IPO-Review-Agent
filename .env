# Environment Variables for IPO Review Agent
# Copy this file to .env and fill in your API keys

# Alpha Vantage API for financial data
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# News API for market news and sentiment analysis
NEWS_API_KEY=your_news_api_key_here

# Finnhub API for additional market data
FINNHUB_API_KEY=your_finnhub_api_key_here

# LLM APIs for advanced prospectus analysis
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Application settings
DEBUG=True
LOG_LEVEL=INFO

# Database settings (if using local database)
DATABASE_URL=sqlite:///ipo_analysis.db
